<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="css/animate.css"/>
    <link rel="stylesheet" href="framework/bootstrap/css/bootstrap.css"/>
    <link rel="stylesheet" href="framework/bootstrap/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="css/animate.css" type="text/css"/>
    <link rel="stylesheet" href="css/style.css" type="text/css"/>
    <link rel="stylesheet" href="framework/Font-Awesome-master/css/font-awesome.min.css"/>
    <title>logistics</title>
</head>
<body>
<!--oncontextmenu=self.event.returnValue=false onselectstart="return false"-->
<div ui-view>
</div>
<!--<div ng-include="'tpls/common/loading.html'"></div>-->
<div ng-include="'tpls/common/right-menu.html'"></div>

<!-- framework -->
<script src="framework/angular/angular.js"></script>
<script src="framework/angular/angular-ui-router.js"></script>
<script src="framework/angular/ui-bootstrap-tpls-0.14.3.min.js"></script>
<!--app-->
<script src="js/config.js"></script>
<!-- controller -->
<script src="js/mainCtrl.js"></script>
<script src="js/controllers/bisController.js"></script>
<!-- service -->
<script src="js/factory.js"></script>
<!-- directive -->
<script src="js/directive.js"></script>
<!-- filter -->
<script src="js/filter.js"></script>
<script>
    setInterval(function () {
        var tag = document.body.querySelector('.tagCurrent');
        if (tag && tag.querySelector('.tab-pane')) {
            var window_height = window.innerHeight;
            var body_height = document.body.clientHeight;
            var sub_height = window_height - body_height;
            var table_height = tag.querySelector('.tabCurrent').querySelector('.grid_body').offsetHeight;
            tag.querySelector('.tabCurrent').querySelector('.grid_body').style.height = (table_height + sub_height).toString() + "px";
        }
        else {
            if (tag && tag.querySelector('.grid_body')) {
                var window_height = window.innerHeight;
                var body_height = document.body.clientHeight;
                var sub_height = window_height - body_height;
                var table_height = tag.querySelector('.grid_body').offsetHeight;
                tag.querySelector('.grid_body').style.height = (table_height + sub_height).toString() + "px";
            }
        }
    }, 1)
    document.addEventListener("click", function () {
        window.showContextMenu = false;
    })
</script>

</body>
</html>