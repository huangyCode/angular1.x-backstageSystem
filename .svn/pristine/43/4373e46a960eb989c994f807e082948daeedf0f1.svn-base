<ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a>111</a></li>
    <li role="presentation"><a>222</a></li>
</ul>
<div ng-show="" ng-controller="bisWarehouse">
    <div class="table-choose">
        <form ng-submit="warehouse.go()" class="form-inline m-b-md">
            <div class="input-group input-magin">
                <span class="input-group-addon">编号</span>
                <input type="text" ng-model="param.id" class="form-control">
            </div>
            <div class="input-group input-magin">
                <span class="input-group-addon">名字</span>
                <input type="text" ng-model="param.name" class="form-control">
            </div>
            <div class="input-group input-magin">
                <span class="input-group-addon">类型</span>
                <input type="text" ng-model="param.type" class="form-control">
            </div>
            <div class="input-group input-magin">
                <span class="input-group-addon">状态</span>
                <select ng-model="param.status" class="form-control">
                    <option value="ENABLED">生效</option>
                    <option value="DISABLED">失效</option>
                </select>
            </div>
            <div ng-click="warehouse.go()" class="btn btn-primary input-magin">搜索</div>
        </form>
        <!--选择操作-->
        <div class="m-l-md btn-group dropdown" uib-dropdown>
            <button class="btn btn-default dropdown-toggle" uib-dropdown-toggle>
                操作选择 <span class="caret"></span>
            </button>
            <ul class="dropdown-menu">
                <!--<li ng-repeat="ctrl in ctrlMenu"><a ng-click="ctrl.click()" ng-bind="ctrl.name"></a></li>-->
                <li><a ng-click="search()">条件查询</a></li>
                <li><a ng-click="asearch('lg')">高级查询</a></li>
                <li><a ng-click="delete()" ng-class="{'disabled':!check()}">删除</a></li>
                <li><a ng-click="detail()" ng-class="{'disabled':!detailCheck()}">查看详情</a></li>
                <li><a ng-click="enable()" ng-class="{'disabled':!enableCheck()}">生效</a></li>
                <li role="separator" class="divider"></li>
                <li><a href="#">导出</a></li>
                <li><a href="#">导入</a></li>
            </ul>
        </div>
        <!--  end  -->
        <div class="btn-group">
            <button class="btn btn-info" ng-disabled="!check()">删除
                <span class="glyphicon glyphicon-minus"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-info">添加
                <span class="glyphicon glyphicon-plus"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-info">修改
                <span class="glyphicon glyphicon-pencil"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-success">打开
                <span class="glyphicon glyphicon-ok"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-danger">关闭
                <span class="glyphicon glyphicon-remove"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-success" ng-disabled="!enableCheck()">生效
                <span class="glyphicon glyphicon-play"></span>
            </button>
        </div>
        <div class="btn-group">
            <button class="btn btn-danger">失效
                <span class="glyphicon glyphicon glyphicon-stop"></span>
            </button>
        </div>
        <div class="btn-group" style="float: right">
            <button class="btn btn-default" style="margin-top:2px" ng-click="warehouse.ctrlTableShow()">修改列表显示
                <span class="fa fa-gear"></span>
            </button>
        </div>
    </div>
    <div pageutil="warehouse" bodyurl="testJson/warehouse.json" headurl="testJson/ctrlTable.json"
         filter="warehousefilter" param="param">
    </div>
</div>
<!-- -------------------------------操作弹框--------------------------------------- -->
<!-- 删除页面 -->
<script type="text/ng-template" id="delete.html">
    <div class="modal-header">
        <h3 class="modal-title">删除</h3>
    </div>
    <div class="modal-body">
        确认删除？
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary" ng-click="ok()">OK</button>
        <button class="btn btn-warning" ng-click="cancel()">Cancel</button>
    </div>
</script>
<!---搜索-->
<script type="text/ng-template" id="search.html">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                aria-hidden="true">&times;</span></button>
        <h4>条件查询</h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <!-- 行 -->
            <div class="form-group">
                <div class="col-md-6">
                    <label class="col-sm-3 control-label">id:</label>

                    <div class="col-md-9">
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button type="button" class="btn btn-default dropdown-toggle">大于 <span
                                        class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="#">大于</a></li>
                                    <li><a href="#">小于</a></li>
                                    <li><a href="#">等于</a></li>
                                </ul>
                            </div>
                            <!-- /btn-group -->
                            <input type="text" class="form-control">
                        </div>
                        <!-- /input-group -->
                    </div>
                </div>
                <div class="col-md-6">
                    <label class="col-md-4 control-label">id:</label>

                    <div class="col-md-8">
                        <input type="text" class="form-control">
                    </div>
                </div>
            </div>
            <!-- 行 -->
            <div class="form-group">
                <div class="col-xs-6">
                    <label class="control-label col-xs-4">name:</label>

                    <div class="col-xs-8">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
            <div class="form-group">
                <div class="col-md-6">
                    <label class="col-md-4 control-label">code:</label>

                    <div class="col-md-8">
                        <input type="text" class="form-control ">
                    </div>
                </div>
                <div class="col-md-6">
                    <label class="col-md-4 control-label">code:</label>

                    <div class="col-md-8">
                        <input type="text" class="form-control">
                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <center>
            <input type="button" class="btn btn-primary" value="提交">
            <input type="button" class="btn btn-default" data-dismiss="model" value="取消">
        </center>
    </div>
</script>
<!-- 高级查询 -->
<script type="text/ng-template" id="advancedSearch.html">
    <div class="modal-header">
        <h3 class="modal-title">高级查询</h3>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <div class="form-group">
                <div class="col-md-6">
                    <label class="col-md-3 control-label">编号:</label>

                    <div class="col-md-9">
                        <input type="text" class="form-control ">
                    </div>
                </div>
                <div class="col-md-6">
                    <label class="col-md-3 control-label">名字:</label>

                    <div class="col-md-9">
                        <input type="text" class="form-control ">
                    </div>
                </div>
            </div>
            <div class="input-group input-magin">
                <span class="input-group-addon">编号</span>
                <input type="text" class="form-control">
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <center>
            <button class="btn btn-primary" ng-click="ok()">确认查询</button>
            <button class="btn btn-warning" ng-click="cancel()">取消</button>
        </center>
    </div>
</script>
