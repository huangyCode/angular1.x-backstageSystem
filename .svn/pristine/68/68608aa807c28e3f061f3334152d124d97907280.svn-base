<!-- 左边导航栏 （展开）-->
<div class="wrapper" ng-show="wrapperShow">
    <nav class="navbar-default navbar-static-side" role="navigation">
        <div class="sidebar-collapse">
            <ul class="nav m-t-md side-menu">
                <li><span class="nav-label m-l-xs" ng-bind="nowTime"></span>
                    <span class="wrapper-sm-btn" ng-click="wrapperBtn()">
                        <i class="fa fa-lg fa-dedent" style="margin: 3px"></i>
                    </span>
                </li>
            </ul>
            <ul class="nav m-t-md side-menu">
                <li>
                    <div class="input-group" role="search">
                        <input type="text"
                               class="form-control input-sm bg-light no-border no-borders rounded padder"
                               placeholder="导航栏搜索">
                            <span class="input-group-btn">
                                <button class="btn btn-sm bg-light rounded" ng-click="search()">
                                    <i class="fa fa-search"></i>
                                </button>
                             </span>
                    </div>
                </li>
                <li class="animated fadeInDown" ng-class="{'active':arr[$index]}" ng-repeat="list in roleMenu">
                    <a class="nav-one-level" ng-click="toggle(list)">
                        <i class="fa fa-lg m-r-md menu-header fa-bar-chart-o"></i>
                        <span class="nav-label" ng-bind="list.name"></span>
                    </a>
                    <!--<second-level></second-level>-->
                    <ul class="nav nav-second-level" ng-class="{'active':list.active}"
                        ng-style="{height:(list.active?(list.childMenus||[]).length*40:0)+'px'}">
                        <li ng-repeat="list2 in (list.childMenus||[])" ng-click="appendTag(list2)">
                            <a class="second-level" ng-bind="list2.name"></a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>
</div>
<!-- 左边导航栏（收起） -->
<div class="wrapper-sm" ng-show="!wrapperShow">
    <nav class="navbar-default navbar-static-side" role="navigation">
        <div class="sidebar-collapse">
            <ul class="nav m-t-md side-menu">
                <li>
                    <span class="wrapper-lg-btn" ng-click="wrapperBtn()">
                        <i class="fa fa-lg fa-indent" style="margin: 3px"></i>
                    </span>
                </li>
            </ul>
            <ul class="nav m-t-lg side-menu">
                <li class="li-sm animated fadeInDown " ng-class="{'active':arr[$index]}" ng-repeat="list in roleMenu">
                    <a ng-click="toggle(list)">
                        <i class="fa fa-lg  menu-header fa-bar-chart-o"></i>
                        <span class="nav-label"></span>
                    </a>
                    <!--<second-level></second-level>-->
                    <ul class="nav nav-second-level second-sm" ng-class="{'active':list.active}"
                        ng-style="{height:(list.active?(list.childMenus||[]).length*40:0)+'px'}">
                        <li ng-repeat="list2 in (list.childMenus||[])" ng-click="appendTag(list2)">
                            <a ng-bind="list2.name"></a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </nav>
</div>
<!-- 右边详情 -->
